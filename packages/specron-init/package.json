{
  "name": "@specron/init",
  "version": "0.0.36",
  "description": "Project generator for ChainSpin framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "transpile": "npm run clean; tsc",
    "prepare": "npm run transpile",
    "test": "nyc ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/tests/*.test.ts",
      "src/tests/**/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/tests"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^5.0.3",
    "@types/node": "^10.3.2",
    "ava": "^1.0.0-beta.6",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "fs-extra": "^7.0.0"
  },
  "gitHead": "c22050569295e5621b246ff35505aad4661a914d"
}
